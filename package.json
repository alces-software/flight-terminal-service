{
  "name": "flight-terminal-service",
  "version": "0.1.8",
  "description": "A terminal service using child_pty and socket.io-stream for use by terminal.js",
  "engines": {
    "node": "8.12.0"
  },
  "scripts": {
    "test": "jshint app.js public",
    "repl": "local-repl",
    "start": "DEBUG=${DEBUG:-FlightTerminalService*} node lib/app.js"
  },
  "author": "Enno Boland <eb@s01.de>",
  "license": "MIT",
  "dependencies": {
    "child_pty": "^3.0.0",
    "debug": "^3.1.0",
    "dotenv": "^7.0.0",
    "jsonwebtoken": "^8.3.0",
    "node-fetch": "^2.1.2",
    "socket.io": "^1.4.5",
    "socket.io-stream": "^0.9.0",
    "socketio-auth": "^0.1.1",
    "tmp": "^0.0.33",
    "ursa": "^0.9.4"
  },
  "jshintConfig": {
    "eqeqeq": true,
    "quotmark": true,
    "nonew": true,
    "indent": true,
    "latedef": true,
